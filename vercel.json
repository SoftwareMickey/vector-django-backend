{
  "version": 2,
  "builds": [
    {
      "src": "models/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "250mb",
        "pythonVersion": "3.10"
      }
    },
    {
      "src": "models/static/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "models/wsgi.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/models/static/$1"
    }
  ]
}

